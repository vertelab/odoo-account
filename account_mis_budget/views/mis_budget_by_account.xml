<?xml version="1.0" encoding="utf-8" ?>

<odoo>

     <record id="mis_budget_by_account_form_inherit" model="ir.ui.view">
            <field name="name">mis.budget.by.account.form.inherit</field>
            <field name="model">mis.budget.by.account</field>
            <field name="inherit_id" ref="mis_builder_budget.mis_budget_by_account_form_view"/>
            <field name="arch" type="xml">
                <!-- Adding the new field before date_range_id -->
                <field name="date_range_id" position="before">
                    <field name="date_type" string="Date Type"/>
                </field>
            </field>
        </record>

    <record id="model_account_aggregate_action" model="ir.actions.server">
        <field name="name">Aggregate Account Budget</field>
        <field name="model_id" ref="mis_builder_budget.model_mis_budget_by_account"/>
        <field name="binding_model_id" ref="mis_builder_budget.model_mis_budget_by_account"/>
        <field name="binding_view_types">list</field>
        <field name="state">code</field>
        <field name="code">action = records.action_aggregate_account_budget()</field>
    </record>

    <record id="model_budget_aggregate_action" model="ir.actions.server">
        <field name="name">Aggregate Budget</field>
        <field name="model_id" ref="mis_builder_budget.model_mis_budget"/>
        <field name="binding_model_id" ref="mis_builder_budget.model_mis_budget"/>
        <field name="binding_view_types">list</field>
        <field name="state">code</field>
        <field name="code">action = records.action_aggregate_budget()</field>
    </record>
</odoo>
